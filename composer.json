{
  "name": "lafourchette/fixtures-bundle",
  "type": "symfony-bundle",
  "description": "Best use for nelmio/alice fixtures",
  "keywords": ["fixtures", "nelmio", "yml"],
  "license": "MIT",
  "authors": [
    {
      "name": "MikaÃ«l Labrut",
      "email": "labrut@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/framework-bundle": "~2.6|~3.0",
    "symfony/validator": "~2.5|~3.0",
    "doctrine/orm": "~2.2,>=2.2.3",
    "doctrine/inflector": "~1.0",
    "doctrine/doctrine-bundle": "~1.2",
    "doctrine/data-fixtures": "dev-master",
    "nelmio/alice": "@stable",
    "doctrine/doctrine-fixtures-bundle": "2.2.*@dev"
  },
  "require-dev": {
    "behat/behat": "~3.0",
    "behat/symfony2-extension": "~2.0",
    "behat/mink": "~1.5",
    "behat/mink-extension": "~2.0",
    "behat/mink-browserkit-driver": "~1.1",
    "behatch/contexts": "~2.1",
    "phpunit/phpunit": "~4.6",
    "symfony/finder": "~2.7",
    "symfony/var-dumper": "~2.7",
    "matthiasnoback/symfony-dependency-injection-test": "0.*"
  },
  "autoload": {
    "psr-4": { "LaFourchette\\FixturesBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
